{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.1",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "artesaos/seotools": "^1.2",
        "automattic/woocommerce": "^3.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "doctrine/dbal": "^3.7",
        "fruitcake/php-cors": "^1.2",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "laraveldaily/laravel-invoices": "^3.0",
        "mews/purifier": "^3.4",
        "nwidart/laravel-modules": "^10.0",
        "paytabscom/laravel_paytabs": "^1.4",
        "spatie/image-optimizer": "^1.7",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-permission": "^5.11",
        "spatie/laravel-sitemap": "^6.3",
        "spatie/laravel-translatable": "^6.5",
        "spatie/laravel-webhook-server": "^3.6",
        "stancl/tenancy": "^3.7",
        "yajra/laravel-datatables-oracle": "^10.3.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "kkomelin/laravel-translatable-string-exporter": "^1.21",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Plugins\\": "plugins/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Helpers/funtions.php",
            "app/Helpers/module-helper.php",
            "app/Helpers/theme-helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
